---
import V2Layout from "@/layouts/PanelViewLayout.astro";
import { ItemName } from "@/components/utilitified_decorations/WorkNavbar.astro";
import NavLabelElement from "@/components/utilitified_decorations/NavigatingItem";
export const replacement = [
      await NavLabelElement({
            children: "Add New Project",
            active: true,
      }),
];

import ConnectionCard from "@/components/project/BlockchainRecordingPanel";
import InfoCard from "@/components/project/BlockchainRecordingHowtoPanel";
---

<V2Layout activeLink={ItemName.ProjectName} additional={replacement}>
      <ConnectionCard slot="center" />
      <InfoCard slot="right" />
</V2Layout>
