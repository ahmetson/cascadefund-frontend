---
import V2Layout from "@/layouts/PanelViewLayout.astro";
import MainMenu from "@/components/menu/MenuPanel";
import IssuesHowToInfo from "@/components/influencer/IssuesHowToInfoPanel";
import QuestPanel from "@/components/influencer/QuestPanel";
import IssueBoard from "@/components/issue/IssueListPanel";
import QuestHowToInfo from "@/components/influencer/QuestHowToInfoPanel";
import VotingPower from "@/components/influencer/VotingPowerPanel";
---

<V2Layout>
  <MainMenu slot="left" activeMenuItem="iwork" client:load />
  <IssuesHowToInfo slot="left" />
  <QuestPanel slot="center" />
  <IssueBoard slot="center" />
  <QuestHowToInfo slot="right" />
  <VotingPower slot="right" />
</V2Layout>
