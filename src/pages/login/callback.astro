---
import V2Layout from "@/layouts/PanelViewLayout.astro";
import { ItemName } from "@/components/utilitified_decorations/LandingNavbar.astro";

import Loading from "@/components/utilitified_decorations/LoadingPanel";

const sectionInfo = {
  title: "Successful Authentication",
  children: "Please wait for automatic redirection",
  gotoLink: "/login/getting-started",
  gotoLabel: "Getting Started",
};
---

<V2Layout hideLinks={Object.values(ItemName)} subtitle="Wait a bit please...">
  <Loading slot="center" client:load {...sectionInfo} />
</V2Layout>
