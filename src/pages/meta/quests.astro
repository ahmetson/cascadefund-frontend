---
import "@/styles/index.css";
import Header from "@/components/landing/Header";
import Hero from "@/components/landing/Hero";
import Vision from "@/components/landing/Goal";
import HowItWorks from "@/components/landing/WhatIsIt";
import Features from "@/components/landing/Preview";
import Pricing from "@/components/landing/CascadeFunding";
import JoinUs from "@/components/landing/JoinUs";
import Team from "@/components/landing/Maintainers";
import Footer from "@/components/landing/Footer";
import Layout from "@/layouts/Layout.astro";
import { Accent } from "@/scripts/data";
---

<Layout>
  <Header client:load />
  <main>
    <Hero client:load accent={Accent.questSystemAccent} />
    <HowItWorks client:load accent={Accent.questSystemAccent} />
    <Features client:load />
    <Vision client:load />
    <Pricing client:load />
    <JoinUs client:load />
    <Team client:load />
  </main>
  <Footer client:load />
</Layout>
