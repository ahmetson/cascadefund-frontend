---
import "../../styles/v2/landing_page.css";
import V2Layout from "../../layouts/V2Layout.astro";
import Header from '../../components/utilitified_decorations/Header'
import Footer from '../../components/utilitified_decorations/Footer'

import HeroSection from '../../components/v2/1.1_landing_page/HeroSection'
import WhyMaintainersStruggle from '../../components/v2/1.1_landing_page/WhyMaintainersStruggle'
import HowItWorks from '../../components/v2/1.1_landing_page/HowItWorks'
import BeAFoundingMaintainer from '../../components/v2/1.1_landing_page/BeAFoundingMaintainer'
import AraProject from '../../components/v2/1.1_landing_page/AraProject'
import FAQ from '../../components/v2/1.1_landing_page/FAQ'
import CallToAction from '../../components/v2/1.1_landing_page/CallToAction'
import { NavigationName } from "../../components/utilitified_decorations/HeaderRight";
---

<V2Layout>
    <div class="min-h-screen bg-white">
      <Header landingPageTopMenu={true} activeLink={NavigationName.MaintainerLanding} />
      <main>
        <HeroSection />
        <WhyMaintainersStruggle />
        <HowItWorks />
        <BeAFoundingMaintainer />
        <AraProject />
        <FAQ />
        <CallToAction />
      </main>
      <Footer />
    </div>
</V2Layout>