---
import "@/styles/v2/influencer_landing_page.css";
import V2Layout from "@/layouts/PanelViewLayout.astro";

import HeroSection from '@/components/v2/influencer_landing_page/HeroSection'
import FundingTransparency from '@/components/v2/project/FundingTransparencyPanel'
import WhySupportSection from '@/components/v2/project/WhySupportSectionPanel'
import TopInfluencers from '@/components/v2/project/TopInfluencersPanel'
import HighlightedIssues from '@/components/v2/project/HighlightedIssuesPanel'

---
<V2Layout landingPageTopMenu={true} cascadeLogoSubtitle="Become an Influencer and Network" >
        <HeroSection slot="hero" />
            <FundingTransparency slot="center" />
            <WhySupportSection slot="center" />
          <TopInfluencers slot="right" />
          <HighlightedIssues slot="right" />
</V2Layout>