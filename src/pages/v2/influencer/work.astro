---
import V2Layout from "@/layouts/PanelViewLayout.astro";
import MainMenu from "@/components/utilitified_decorations/MenuPanel"
import IssuesHowToInfo from '@/components/v2/influencer/IssuesHowToInfoPanel'
import QuestPanel from '@/components/v2/influencer/QuestPanel'
import IssueBoard from '@/components/v2/influencer/IssueBoardPanel'
import QuestHowToInfo from '@/components/v2/influencer/QuestHowToInfoPanel'
import VotingPower from '@/components/v2/influencer/VotingPowerPanel'

---
<V2Layout landingPageTopMenu={false}>
  <MainMenu slot="left" activeMenuItem="iwork" client:load />
  <IssuesHowToInfo slot="left" />
  <QuestPanel slot="center" />
  <IssueBoard slot="center" />
  <QuestHowToInfo slot="right" />
  <VotingPower slot="right" />
</V2Layout>