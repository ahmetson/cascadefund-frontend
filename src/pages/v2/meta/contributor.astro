---
import "../../../styles/v2/1.header.1_contributor.css";
import V2Layout from "../../../layouts/V2Layout.astro";

import Header from '../../../components/utilitified_decorations/Header'
import Footer from '../../../components/utilitified_decorations/Footer'

import HeroSection from '../../../components/v2/1.header.1_contributor/HeroSection'
import ChallengesSection from '../../../components/v2/1.header.1_contributor/ChallengesSection'
import HowItWorksSection from '../../../components/v2/1.header.1_contributor/HowItWorksSection'
import DevelopmentSection from '../../../components/v2/1.header.1_contributor/DevelopmentSection'
import StatsSection from '../../../components/v2/1.header.1_contributor/StatsSection'
import { NavigationName } from "../../../components/utilitified_decorations/HeaderRight";

---
<V2Layout>
    <div class="min-h-screen bg-white">
      <Header landingPageTopMenu={true} activeLink={NavigationName.ContributorLanding} cascadeLogoSubtitle="For open source contributors" />
      <HeroSection />
      <ChallengesSection />
      <HowItWorksSection />
      <DevelopmentSection />
      <StatsSection />
      <Footer />
    </div>
</V2Layout>