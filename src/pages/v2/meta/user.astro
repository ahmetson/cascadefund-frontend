      <Header landingPageTopMenu={true} activeLink={NavigationName.UserLanding} cascadeLogoSubtitle="For open source users (a.k.a influencers)" />
      <HeroSection />
      <ChallengesSection />
      <SolutionSection />
      <HowItWorksSection />
      <UniqueFeatureSection />
      <WhySupportSection />
      <DevelopmentSection />
      <StatsSection />
      <Footer />
    </div>
</V2Layout>