---
import "../../../styles/v2/1.header.3_work.css";
import V2Layout from "../../../layouts/V2Layout.astro";
import Header from '../../../components/utilitified_decorations/Header'
import Footer from '../../../components/utilitified_decorations/Footer'
import { NavigationName } from "../../../components/utilitified_decorations/HeaderRight";

import HeroSection from '../../../components/v2/1.header.3_work/HeroSection'
import CastSection from '../../../components/v2/1.header.3_work/CastSection'
import StorySection from '../../../components/v2/1.header.3_work/StorySection'
import MaintainerSection from '../../../components/v2/1.header.3_work/MaintainerSection'
import RatingsSection from '../../../components/v2/1.header.3_work/RatingsSection'
import VideoSection from '../../../components/v2/1.header.3_work/VideoSection'
import CTASection from '../../../components/v2/1.header.3_work/CTASection'
---

<V2Layout>
  <div class="min-h-screen bg-white">
      <Header landingPageTopMenu={true} activeLink={NavigationName.HowItWork} cascadeLogoSubtitle="How does CascadeFund work?" />
      <main>
        <HeroSection />
        <CastSection />
        <StorySection />
        <MaintainerSection />
        <RatingsSection />
        <VideoSection />
        <CTASection />
      </main>
      <Footer />
    </div>
</V2Layout>